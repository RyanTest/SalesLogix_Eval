    <property xsi:type="OrmCustomProperty" id="ced040df-46ed-40af-925b-cabd690cfe00" lastModifiedUtc="0001-01-01T00:00:00" name="DaysOpen" audited="false" hasGet="true" hasSet="false" returnType="System.Int32">
      <ExtendedPropertiesCollection>
        <extendee type="Sage.Platform.Orm.Entities.OrmCustomProperty" />
      </ExtendedPropertiesCollection>
      <SystemDataType guid="6380b485-b76f-4cb2-953c-2008f39ff781">
        <CustomPropertyDataType />
      </SystemDataType>
      <code><![CDATA[public virtual System.Int32 DaysOpen
{
    get
    {
           int days =0;
           DateTime openDate;
		   DateTime closeDate;
           try
           {
               if(this.DateOpened != null)
			   {    
			      openDate = (DateTime)this.DateOpened;
			   }
			   else
			   {
				  openDate = (DateTime)this.CreateDate;
			   }
			
			   if (this.Status == "Open") 
			   {
                   closeDate = System.DateTime.UtcNow; 
			   }
			   else //All other status
               {
                    if(this.ActualClose != null)
				    {    
					     closeDate = (DateTime)this.ActualClose;
				    }
					else
					{
					     closeDate = System.DateTime.UtcNow;
					}
               }
               
			   TimeSpan timeSpan = new TimeSpan(closeDate.Ticks - openDate.Ticks);
               days =(int) timeSpan.TotalDays;
           }
           catch
           {
               days = 0;
           }
           return days;
    }
}

]]></code>
    </property>
    <property xsi:type="OrmCustomProperty" id="a4f03692-303c-4ca1-b72c-7c6c700df84f" lastModifiedUtc="0001-01-01T00:00:00" name="DaysSinceLastActivity" audited="false" hasGet="true" hasSet="false" returnType="System.Int32">

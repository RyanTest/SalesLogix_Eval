      </ExtendedPropertiesCollection>
      <SystemDataType guid="6380b485-b76f-4cb2-953c-2008f39ff781">
        <CustomPropertyDataType />
      </SystemDataType>
      <code><![CDATA[public virtual System.Int32 DaysSinceLastActivity
{
	get
    {
     int days = 0;
     DateTime modDate;
	 DateTime currTime;

     try
      {
          if (this.ModifyDate != null)
          {
			 currTime = System.DateTime.UtcNow;
             modDate = (DateTime)this.ModifyDate;
             TimeSpan ts = new TimeSpan(currTime.Ticks - modDate.Ticks);
             days = (int)ts.Days;
          }
      }
      catch
      {
         days = 0;
      }
      return days;
     }
}

]]></code>

    <property xsi:type="OrmCustomProperty" id="4e98a1ce-3262-4dad-990f-90308ca1dcd1" lastModifiedUtc="0001-01-01T00:00:00" name="CityStatePostal" audited="false" hasGet="true" hasSet="false" returnType="System.String">
      <ExtendedPropertiesCollection>
        <extendee type="Sage.Platform.Orm.Entities.OrmCustomProperty" />
      </ExtendedPropertiesCollection>
      <SystemDataType guid="6380b485-b76f-4cb2-953c-2008f39ff781">
        <CustomPropertyDataType />
      </SystemDataType>
      <code><![CDATA[public virtual System.String CityStatePostal
{
    get
    {   
		String strCityStatePostal;
		/* CityStatePostal (e.g. "Scottsdale, AZ 85258"). */
        strCityStatePostal = String.Format("{0}{1}{2}",
			/* Start the string with "City" if City is defined. */
            (!String.IsNullOrEmpty(this.City) ? this.City : ""),
			/* Include State in the form ", State"; if City is undefined just use "State". */
            (!String.IsNullOrEmpty(this.State) ? (String.Format("{0}{1}", (!String.IsNullOrEmpty(this.City) ? ", " : ""), this.State)) : ""),
            /* Include PostalCode in the form " PostalCode" if it is defined. */
		    (!String.IsNullOrEmpty(this.PostalCode) ? (String.Format(" {0}", this.PostalCode)) : "")).Trim();
        /* If strCityStatePostal begins with "," then remove the comma; otherwise, just return strCityStatePostal. */
		return (!strCityStatePostal.StartsWith(",")) ? strCityStatePostal :
            strCityStatePostal.TrimStart(",".ToCharArray()).Trim();
	}
}
]]></code>
    </property>
